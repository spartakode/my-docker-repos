server{
    listen 80;
    client_max_body_size 20m;
    location /{
        root /usr/share/nginx/html/staticcms;
        index index.html;
    }

    location /blogadmin {
        client_max_body_size 20m;
        proxy_pass http://127.0.0.1:5000;
        proxy_set_header Host $host:8888;
    }
}
